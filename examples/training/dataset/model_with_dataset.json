{   
    "datasets": {
        "training": {
            "filename": "examples/training/dataset/training_ds.tfrecords",
            "size": 100000,
            "batch":10,
            "steps":1000000
        }
    },
    "inputs":{
        "features":[
            {
                "shape": [1],
                "key": "val",
                "dtype": "tf.float32"
            }
        ],
        "labels": {
            "shape": [1],
            "key": "label",
            "dtype": "tf.float32"
        }        
    },           
    "model": {
        "input": "val",
        "layers": [
            {
                "type":"dense",
                "units": 100,
                "activation":"tf.nn.relu"
            },
            {
                "type":"dense",
                "units": 100,
                "activation":"tf.nn.relu"
            },
            {
                "type":"dense",
                "units": 10,
                "activation":"tf.nn.relu"
            },
            {
                "type":"dense",
                "units": 1
            }
        ],
        "output":"logits"
    }    
}

